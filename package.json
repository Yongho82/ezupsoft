{
  "name": "ezup",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:main && npm run build:subapps && npm run copy:subapps",
    "build:main": "vite build",
    "build:subapps": "npm run build:live && npm run build:pdf",
    "build:live": "cd live_editor && npm run build",
    "build:pdf": "cd pdf_studio && npm run build",
    "copy:subapps": "npm run copy:live && npm run copy:pdf",
    "copy:live": "xcopy live_editor\\dist\\* dist\\live_editor\\ /E /I /Y",
    "copy:pdf": "xcopy pdf_studio\\dist\\* dist\\pdf_studio\\ /E /I /Y",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "lucide-react": "^0.554.0",
    "react-router-dom": "^7.9.6",
    "@react-oauth/google": "^0.12.2"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
